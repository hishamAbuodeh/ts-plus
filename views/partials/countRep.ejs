<div id="demo-modal" class="modal-v">
    <div class="report__content">
      <div id="closeDiv">
        <button id='close'>X</button>
      </div>
        <div class="container py-5">
            <div class="row py-5">
            <div class="col-lg-10 mx-auto">
                <div class="card rounded shadow border-0">
                <div class="card-body p-5 bg-white rounded">
                    <div class="table-responsive">
                    <table id="example" style="width:100%" class="table table-striped table-bordered">
                        <thead>
                        <tr>
                            <th>No.</th>
                            <th>Item Code</th>
                            <th>Item Name</th>
                            <th>Barcode</th>
                            <th>Qty</th>
                            <th>Unit</th>
                            <!-- <th>Note</th> -->
                        </tr>
                        </thead>
                        <tbody id="tbody">
                        <% for(let i = 0; i < data.length; i++){ const item = data[i] %>
                            <tr class="trEfects hide" id="tr<%= item.id %>">
                                <td><%= i + 1 %></td>
                                <td id="td1-<%= item.id %>"><%= item.ItemCode %></td>
                                <td id="td2-<%= item.id %>"><%= item.ItemName %></td>
                                <td id="td3-<%= item.id %>"><%= item.CodeBars %></td>
                                <td id="td5-<%= item.id %>"><%= item.Qnty %></td>      
                                <td id="td7-<%= item.id %>"><%= item.BuyUnitMsr %></td>
                                <!-- <td id="td4-<%= item.id %>"><%= item.Note %></td> -->
                            </tr>
                            <% } %>
                        </tbody>
                    </table>
                    </div>
                </div>
                </div>
            </div>
            </div>
        </div>
    </div>
</div>        