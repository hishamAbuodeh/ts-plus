<div id="demo-modal" class="modal-v">
  <div class="report__content">
    <% if(page == "report"){ %>
    <div id="closeDiv2">
      <botton id="excel" class="btu"  style="margin-left: 30px;"><p class="para">Excel</p></botton>
      <button id='close'>X</button>
    </div>
    <% }else{ %>
      <div id="closeDiv1">
        <button id='close'>X</button>
      </div>
    <% } %>
    <div class="container py-5">
        <div class="row py-5">
          <div class="col-lg-10 mx-auto">
            <div class="card rounded shadow border-0">
              <div class="card-body p-5 bg-white rounded">
                <div class="table-responsive">
                  <table id="example" style="width:100%" class="table table-striped table-bordered">
                    <thead>
                      <tr>
                        <th>No.</th>
                        <th style="display:none;">Barcode</th>
                        <th style="display:none;">ConvFactor</th>
                        <th>Item Code</th>
                        <th>Item Name</th>
                        <th>Branch</th>
                        <% if(page == "report"){ %>
                          <th>Avg. Daily Sale</th>
                        <% } %>
                        <th>On Hand</th>
                        <th>Min</th>
                        <th id="max-th">Max</th>
                        <th>Order</th>
                        <th>Price</th>
                        <th>Unit</th>
                        <!-- <th id="toggleSave">Save</th> -->
                      </tr>
                    </thead>
                    <% if(results) { %>
                    <tbody id="tbody">
                      <% for(let i=0; i < results.length; i++){ const item = results[i] %>
                        <tr class="trEfects">
                          <td><%= i + 1 %></td>
                          <td style="display:none;"><%= item.CodeBars %></td>
                          <td style="display:none;"><%= item.ConvFactor %></td>
                          <td><%= item.ItemCode %></td>
                          <td><%= item.ItemName %></td>
                          <td><%= item.ListName %></td>
                          <% if(page == "report"){ %>
                            <td><%= item.AvgDaily %></td>
                          <% } %>
                          <td><%= item.OnHand %></td>
                          <td><%= item.MinStock %></td>
                          <td><%= item.MaxStock %></td>
                          <td style="text-align: center;"><div><%= item.Order %></div></td>
                          <td><%= item.Price %></td>
                          <td ><%= item.BuyUnitMsr %></td>
                        </tr>
                      <% }; %>
                    </tbody>
                    <% } %>
                  </table>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
</div>